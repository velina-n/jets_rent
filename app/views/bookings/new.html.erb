<div class="container mt-5">
  <h1>New Booking</h1>


<div class='container mt-5'>
  <%= simple_form_for [@jet, @booking] do |f| %>
  <%= f.input :airport %>
  <%= f.input :start_date, placeholder: "yyyy-mm-jj" %>
  <%= f.input :end_date, placeholder: "yyyy-mm-jj" %>
  <%= f.input :special_requests %>
  <%= f.submit%>

  <% end %>
</div>
